{
  "name": "servonapp",
  "productName": "ServonApp",
  "version": "0.3.0",
  "description": "The Application from Servon",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build-installer": "electron-builder"
  },
  "build": {
    "appId": "servon-app",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/assets/images/logo.ico"
    },
    "nsis": {
      "installerIcon": "src/assets/images/logo.ico",
      "uninstallerIcon": "src/assets/images/logo.ico",
      "uninstallDisplayName": "ServonApp",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "electron": "30.1.0"
  },
  "keywords": [],
  "author": {
    "name": "Blonicx",
    "email": "belte_YT@outlook.com"
  },
  "license": "MIT",
  "dependencies": {
    "@vercel/speed-insights": "^1.0.12",
    "axios": "^1.7.2",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "mongodb": "^6.8.0",
    "mongoose": "^8.4.5",
    "webrtc": "^1.14.1"
  }
}
